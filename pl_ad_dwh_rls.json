                    "retry": 2,
                    "retryIntervalInSeconds": 120,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "source": {
                        "type": "SqlDWSource",
                        "sqlReaderQuery": "SELECT [id]\n      ,[displayName]\n      ,[mail]\n  FROM [temp].[rlsGroups]",
                        "queryTimeout": "02:00:00",
                        "partitionOption": "None"
                    },
                    "dataset": {
                        "referenceName": "ds_dimtable_RH",
                        "type": "DatasetReference",
                        "parameters": {
                            "schema_name": "temp",
                            "table_name": "rlsGroups"
