                            "treatBooleanAsNumber": false
                        }
                    }
                },
                "inputs": [
                    {
                        "referenceName": "ds_ad_staging_getGroups",
                        "type": "DatasetReference"
                    }
                ],
                "outputs": [
                    {
                        "referenceName": "ds_dimtable_RH",
                        "type": "DatasetReference",
                        "parameters": {
                            "schema_name": "temp",
                            "table_name": "rlsGroups"
                        }
                    }
                ]
            },
            {
